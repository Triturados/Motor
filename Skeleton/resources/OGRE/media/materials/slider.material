material Heal_bg{
	technique{
		pass{
			lighting off
			depth_check off
			scene_blend alpha_blend
			texture_unit {
				texture Heal_bg.png
				colour_op modulate

				scale 0.1 1
				scroll 0.5 0
			}
		}
	}
}

